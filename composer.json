{
    "name": "tiderjian/sysconf",
    "description": "System config extension for laravel-admin",
    "type": "laravel-admin,config",
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "~5.5",
        "encore/laravel-admin": "~1.6",
        "spatie/eloquent-sortable": "~3.4",
        "jxlwqq/json-editor": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.4",
        "laravel/laravel": "~5.5",
        "laravel/browser-kit-testing": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Encore\\Admin\\Sysconf\\": "src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "tiderjian",
            "email": "164710115@qq.com"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Encore\\Admin\\Sysconf\\SysconfServiceProvider"
            ]
        }
    }
